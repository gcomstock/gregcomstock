{
  "name": "gregcomstock",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://gcomstock.github.io/",
  "devDependencies": {
    "gh-pages": "^0.12.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "react-scripts": "0.8.5"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  },
  "scripts": {
    "build-css": "node-sass src/scss/base.scss -o src/css",
    "watch-css": "npm run build-css && node-sass src/scss/base.scss -o src/css --watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  }
}
